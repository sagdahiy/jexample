            steps {
                echo 'test World'
            }    }

        stage('Test') {
            steps {
                echo 'test World'
            }    }
        stage('final test') {
            steps {
                echo 'test World'
            }    }
        stage('Release') {
            steps {
                echo 'release World'
            }    }
        
    }
    }
